#! /usr/bin/env python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pysorcery==0.1.1','cli','summon'
__requires__ = 'pysorcery==0.1.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pysorcery==0.1.1', 'sorcery_summon', 'summon')()
        )
